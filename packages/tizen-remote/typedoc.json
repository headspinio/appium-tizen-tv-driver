{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["./lib/index.js"],
  "excludePrivate": true,
  "name": "tizen-remote",
  "includeVersion": true,
  "tsconfig": "./tsconfig.json",
  "out": "docs",
  "plugin": ["typedoc-plugin-markdown"],
  "excludeExternals": true,
  "cleanOutputDir": true,
  "readme": "none",
  "disableSources": true,

  "hideBreadcrumbs": true,
  "hideInPageTOC": true,
  "entryDocument": "index.md"
}
