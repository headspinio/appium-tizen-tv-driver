    "test:unit": "cross-env DEBUG=\"tizen-remote*\" mocha \"test/unit/*.spec.js\"",
    "test:types": "tsd"
  },
  "dependencies": {
    "@appium/strongbox": "0.3.2",
    "@humanwhocodes/env": "2.2.2",
    "@types/node": "18.17.12",
    "@types/ws": "8.5.5",
    "debug": "4.4.0",
    "delay": "4.4.1",
    "got": "11.8.6",
    "lodash": "4.17.21",
    "p-retry": "4.6.2",
    "strict-event-emitter-types": "2.0.0",
    "type-fest": "3.13.1",
    "ws": "8.18.0"
  },
  "tsd": {
    "directory": "test/types"
  },
  "publishConfig": {
    "access": "public"
  }
