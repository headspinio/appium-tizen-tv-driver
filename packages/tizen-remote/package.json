{
  "name": "@headspinio/tizen-remote",
  "version": "0.4.4",
  "description": "Remote control automation for Tizen devices",
  "keywords": [
    "tizen",
    "samsung",
    "remote",
    "rc",
    "appium",
    "automate",
    "automation",
    "control",
    "tv",
    "television"
  ],
  "homepage": "https://github.com/headspinio/appium-tizen-tv-driver/tree/main/packages/tizen-remote#readme",
  "bugs": {
    "url": "https://github.com/headspinio/appium-tizen-tv-driver/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/headspinio/appium-tizen-tv-driver.git",
    "directory": "packages/tizen-remote"
  },
  "license": "Apache-2.0",
  "author": "Christopher Hiller <boneskull@boneskull.com>",
  "main": "index.js",
  "types": "build/lib/tizen-remote.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "files": [
    "lib",
    "build",
    "index.js"
  ],
  "scripts": {
    "build": "npm run build:distfiles",
    "build:distfiles": "babel lib --root-mode=upward --out-dir=build/lib",
    "build:docs": "typedoc",
    "clean": "npm run clean:distfiles",
    "clean:distfiles": "npx rimraf \"build/**/*.js\"",
    "clean:docs": "npx rimraf \"docs/**/*\"",
    "dev": "npm run build -- --watch",
    "lint": "eslint -c ../../.eslintrc --ignore-path ../../.eslintignore",
    "prepare": "npm run build",
    "test": "npm run test:unit",
    "test:e2e": "cross-env BABEL_ENV=test DEBUG=\"tizen-remote*\" mocha \"test/e2e/*.spec.js\" -t 20s",
    "test:smoke": "node index.js",
    "test:unit": "cross-env BABEL_ENV=test DEBUG=\"tizen-remote*\" mocha \"test/unit/*.spec.js\""
  },
  "dependencies": {
    "@babel/runtime": "7.21.0",
    "@humanwhocodes/env": "2.2.2",
    "conf": "10.2.0",
    "debug": "4.3.4",
    "delay": "4.4.1",
    "env-paths": "2.2.1",
    "p-retry": "4.6.2",
    "proper-lockfile": "4.1.2",
    "source-map-support": "0.5.21",
    "ws": "8.12.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
