{
  "name": "appium-tizen-tv-driver",
  "description": "Tizen TV support for Appium",
  "keywords": [
    "appium",
    "tizen",
    "testing",
    "automation",
    "mobile",
    "mobile testing"
  ],
  "version": "0.8.0",
  "author": "Jonathan Lipps <jlipps@headspin.io>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/headspinio/appium-tizen-tv-driver.git",
    "directory": "packages/appium-tizen-tv-driver"
  },
  "bugs": {
    "url": "https://github.com/headspinio/appium-tizen-tv-driver/issues"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "main": "index.js",
  "bin": {},
  "directories": {
    "lib": "lib"
  },
  "files": [
    "index.js",
    "lib",
    "build/lib"
  ],
  "dependencies": {
    "@appium/types": "0.9.0",
    "@babel/runtime": "7.20.13",
    "@headspinio/tizen-remote": "^0.4.4",
    "appium-chromedriver": "5.2.17",
    "asyncbox": "2.9.4",
    "bluebird": "3.7.2",
    "get-port": "5.1.1",
    "got": "11.8.6",
    "keycode": "2.2.1",
    "lodash": "4.17.21",
    "source-map-support": "0.5.21",
    "teen_process": "2.0.2",
    "type-fest": "3.5.6",
    "yargs": "17.6.2"
  },
  "scripts": {
    "build": "npm run build:distfiles",
    "build:distfiles": "babel lib --root-mode=upward --out-dir=build/lib",
    "clean": "npm run clean:distfiles",
    "clean:distfiles": "npx rimraf \"build/**/*.js\"",
    "dev": "npm run build -- --watch",
    "lint": "eslint -c ../../.eslintrc --ignore-path ../../.eslintignore",
    "prepare": "npm run build",
    "test": "mocha \"test/unit/**/*.spec.js\" -t 5s",
    "test:e2e": "mocha \"test/e2e/**/*.e2e.spec.js\" -t 60s",
    "test:smoke": "node index.js"
  },
  "homepage": "https://github.com/headspinio/appium-tizen-tv-driver/tree/main/packages/tizen-remote#readme",
  "appium": {
    "driverName": "tizentv",
    "automationName": "TizenTV",
    "platformNames": [
      "TizenTV"
    ],
    "mainClass": "TizenTVDriver",
    "scripts": {
      "pair-remote": "./build/lib/rc-pair.js"
    }
  },
  "peerDependencies": {
    "appium": "^2.0.0-beta.46"
  }
}
